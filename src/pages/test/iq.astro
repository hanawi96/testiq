---
import TestLayout from '../../layouts/TestLayout.astro';
import IQTestWrapper from '../../components/IQTestWrapper';
import { iqQuestions, testConfig } from '../../data/iqQuestions';

const locale = getLocale('vi');
---

<TestLayout
  title={`${testConfig.title} - ${locale.meta.site_name}`}
  description={testConfig.description}
>
  <IQTestWrapper 
    questions={iqQuestions} 
    timeLimit={testConfig.timeLimit}
    client:load
  />
</TestLayout>